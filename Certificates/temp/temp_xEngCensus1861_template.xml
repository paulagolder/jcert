<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE sourcedocuments  PUBLIC "-//Paul Golder//DTD jcerttemplate//EN" "http://lerot.org/Templates/jcerttemplate2.dtd">

 <sourcedocuments>
   <sourcedocument>
     <template>
       <templateID>Census1861</templateID>
       <version>2</version>
       <stylesheet>../Stylesheets/EngCensus1861_stylesheet.xsl</stylesheet>
       <name>England Census 1861</name>
       <savefilepattern>1861Census/1861Census_cert.xml</savefilepattern>
       <author>Paul A Golder</author>
       <editdate>12 Jan 2013</editdate>
       <type>census</type>
       <country>England</country>
       <period>1861</period>
       <source>ONS image</source>
     </template>

     <certificate objecttype="certificate"   backgroundcolor="#D0F5A9"  >
       <hgroup   >
         <label layout="MIDDLE" >Page</label>
         <input columns="4" target="pageno" />
         <label layout="RIGHT" >Sequence</label>
         <input columns="4" target="sequenceno"  />
       </hgroup>
       <vgroup    >
         <vgroup borderwidth="0" >
           <hgroup layout="center">
             <label  align="center">the undermentioned Houses are situte within the Boundaries of the</label>
           </hgroup>
         </vgroup>
         <table name="heading"  borderwidth="1" >
           <trow borderwidth="0" bordercolor="black" >
             <tcell>
               <label>Parish[or Township] of</label>
             </tcell>
             <tcell>
               <label>
                 <text>City or</text>
                 <text>Municipal Ward of</text>
               </label>
             </tcell>
             <tcell>
               <label>Parliamentary Borough of</label>
             </tcell>
             <tcell>
               <label>Town of</label>
             </tcell>
             <tcell>
               <label>Hamlet or Tything of</label>
             </tcell>
             <tcell>
               <label>Ecclesiastical district of</label>
             </tcell>
           </trow>
           <trow>
             <tcell>
               <input    target="parish"  />
             </tcell>
             <tcell>
               <input    target="ward"  />
             </tcell>
             <tcell>
               <input    target="borough" />
             </tcell>
             <tcell>
               <input    target="town"   />
             </tcell>
             <tcell>
               <input    target="hamlet"  />
             </tcell>
             <tcell>
               <input    target="ecclesiasticalDistrict"   />
             </tcell>
           </trow>
         </table>
         <hgroup/>
         <table  objecttype="people"  >
           <trow    >
             <tcell>
               <label>
                 <text>No of</text>
                 <text>Schedule</text>
               </label>
             </tcell>
             <tcell >
               <label  >
                 <text  >Road Street etc </text>
                 <text>and No or Name </text>
                 <text>of House</text>
               </label>
             </tcell>
             <tcolumn colspan="2" name="trace"  >
               <tcell>
                 <label  align="center" >Houses </label>
               </tcell>
               <trow>
                 <tcell>
                   <label   font-size="8">
                     <text>In-</text>
                     <text>habited </text>
                   </label>
                 </tcell>
                 <tcell>
                   <label  font-size="8">
                     <text>unin-</text>
                     <text>habited</text>
                     <text>(U.). or</text>
                     <text>Building</text>
                     <text>(B.).</text>
                   </label>
                 </tcell>
               </trow>
             </tcolumn>
             <tcell>
               <label>
                 <text>Name and Surname of Each</text>
                 <text>Person</text>
               </label>
             </tcell>
             <tcell>
               <label>
                 <text>Relation</text>
                 <text>to Head</text>
                 <text>of Family</text>
               </label>
             </tcell>
             <tcell>
               <label>Condition</label>
             </tcell>
             <tcolumn  colspan="2" >
               <tcell>
                 <label>Age of</label>
               </tcell>
               <trow>
                 <tcell>
                   <label font-size="8" >Males</label>
                 </tcell>
                 <tcell>
                   <label  font-size="8" >Females</label>
                 </tcell>
               </trow>
             </tcolumn>
             <tcell>
               <label>Rank, Profession, or Occupation</label>
             </tcell>
             <tcell>
               <label>Where Born</label>
             </tcell>
             <tcell>
               <label>
                 <text>Whether</text>
                 <text>Blind or Deaf-</text>
                 <text>-and-Dumb</text>
               </label>
             </tcell>
           </trow>
           <trow   objecttype="person"  rows="10"   >
             <tcell  >
               <input    target="householdReference"  />
             </tcell>
             <tcell>
               <input    target="address"  />
             </tcell>
             <tcell>
               <input    target="inhabitedHouse"  />
             </tcell>
             <tcell>
               <input    target="uninhabitedHouse"  />
             </tcell>
             <tcell>
               <input    target="fullname"  />
             </tcell>
             <tcell>
               <input    target="roleInHousehold"  />
             </tcell>
             <tcell>
               <input    target="maritalStatus"  />
             </tcell>
             <tcell>
               <input  target="age:gender=male"  />
             </tcell>
             <tcell>
               <input    target="age:gender=female"  />
             </tcell>
             <tcell>
               <input    target="occupation"  />
             </tcell>
             <tcell>
               <input    target="birthplace"  />
             </tcell>
             <tcell>
               <input    target="note"  />
             </tcell>
           </trow>
           <trow   >
             <tcell colspan="2" >
               <label >Total of Houses  </label>
             </tcell>
             <tcell>
               <input  target="totalInhabited"  />
             </tcell>
             <tcell>
               <input  target="totalUninhabited"  />
             </tcell>
             <tcell colspan="3" >
               <label>Total of Males and Females </label>
             </tcell>
             <tcell>
               <input  target="totalMales"  />
             </tcell>
             <tcell>
               <input  target="totalFemales"  />
             </tcell>
             <tcell>
             </tcell>
             <tcell>
             </tcell>
             <tcell>
             </tcell>
           </trow>
         </table>
       </vgroup>
     </certificate>
   </sourcedocument>
 </sourcedocuments>
