<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE sourcedocuments  PUBLIC "-//Paul Golder//DTD jcerttemplate//EN" "http://lerot.org/Templates/jcerttemplate.dtd">

 <sourcedocuments>
   <sourcedocument>
     <template>
       <templateID>Census1841</templateID>
       <version>2</version>
       <stylesheet>../Stylesheets/EngCensus1841_stylesheet.xsl</stylesheet>
       <name>England Census 1841</name>
       <author>Paul A Golder</author>
       <editdate>11 Feb 2013</editdate>
       <type>census</type>
       <country>England</country>
       <period>1841</period>
       <source>ONS images</source>
     </template>
     <certificate  objecttype="certificate" backgroundcolor="#D0F5A9"  >
       
       <vgroup    >
         <hgroup   >
           <label layout="MIDDLE" >Page</label>
           <input columns="4" target="pageno" />
           <label layout="RIGHT" >Sequence</label>
           <input columns="4" target="sequenceno"  />
         </hgroup>
         <hgroup   >
           <vgroup   >
             <hgroup name="bor" >
               <label name = "boro" >City or Borough of</label>
               <input columns="20" target="borough"   />
             </hgroup>
             <hgroup   name="par"  >
               <label  >Parish or Township of</label>
               <input columns="20" target="parish"   />
             </hgroup>
           </vgroup>
           <vgroup   layout="RIGHT" >
             <hgroup   >
               <label fontsize="12"  >Enumeration Schedule</label>
             </hgroup>
           </vgroup>
         </hgroup>
       </vgroup>
       <hgroup name ="filler"  height="30"   />
       <table  objecttype="people" borderwidth="1"  >
         <trow>
           <tcell>
             <label>Place</label>
           </tcell>
           <tcolumn   colspan="2" >
             <tcell layout="FILLV"   >
               <label >HOUSES </label>
             </tcell>
             <trow>
               <tcell>
                 <label name="houses"  direction="vertical" fontsize="8">
                   <text>Uninhabited</text>
                   <text>or Building</text>
                 </label>
               </tcell>
               <tcell>
                 <label fontsize="8" direction="vertical">Inhabited</label>
               </tcell>
             </trow>
           </tcolumn>
           <tcell>
             <label  >
               <text>NAMES</text>
               <text fontsize="8">of each Person who obode therein </text>
               <text fontsize="8">the preceding night</text>
             </label>
           </tcell>
           <tcolumn   colspan="2"  >
             <tcell >
               <label align="center"  >
                 <text>AGE </text>
                 <text>and </text>
                 <text>SEX</text>
               </label>
             </tcell>
             <trow  name="sex" layout="FILLV"  >
               <tcell >
                 <label name="males" fontsize="8" direction="vertical" >Males</label>
               </tcell>
               <tcell  >
                 <label fontsize="8" direction="vertical">Females</label>
               </tcell>
             </trow>
           </tcolumn>

           <tcell>
             <label>
               <text>PROFESSION, TRADE</text>
               <text>EMPLOYMENT</text>
               <text>or of</text>
               <text>INDEPENDENT MEANS</text>
             </label>
           </tcell>
           <tcolumn   colspan="2" >
             <tcell>
               <label >Where born</label>
             </tcell>
             <trow>
               <tcell>
                 <label fontsize="8" direction="vertical">
                   <text>Whether born</text>
                   <text>in same County</text>
                 </label>
               </tcell>
               <tcell>
                 <label fontsize="8" direction="vertical">
                   <text>Whether born in</text>
                   <text>England Ireland </text>
                   <text>or Foriegn Parts</text>
                 </label>
               </tcell>
             </trow>
           </tcolumn>
         </trow>
         <trow objecttype="person" rows="10" borderwidth="0" >
           <tcell>
             <input target="place"   />
           </tcell>
           <tcell>
             <input target="uninhabitedHouse"   />
           </tcell>
           <tcell>
             <input target="inhabitedHouse"   />
           </tcell>
           <tcell>
             <input target="name"   />
           </tcell>
           <tcell>
             <input target="age:gender=male"  width="10" />
           </tcell>
           <tcell>
             <input target="age:gender=female"   />
           </tcell>
           <tcell>
             <input target="occupation"   />
           </tcell>
           <tcell>
             <input target="birthplaceThisCounty"   />
           </tcell>
           <tcell>
             <input target="birthplaceOther"   />
           </tcell>
         </trow>
         <trow>
           <tcell>
             <label>Total in Page</label>
           </tcell>
           <tcell>
             <input target="totalUninhabitedHouses"   />
           </tcell>
           <tcell>
             <input target="totalInhabitedHouses"   />
           </tcell>
           <tcell>
           </tcell>
           <tcell>
             <input target="totalMales"   />
           </tcell>
           <tcell>
             <input target="totalFemales"   />
           </tcell>
           <tcell>
           </tcell>
           <tcell>
             <input target="totalThisCounty"   />
           </tcell>
           <tcell>
             <input target="totalOther"   />
           </tcell>
         </trow>
       </table>
     </certificate>
   </sourcedocument>
 </sourcedocuments>
 